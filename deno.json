{
  "imports": {
    "bgutils-js": "npm:bgutils-js@^3.2.0",
    "hono": "npm:hono@^4.7.9",
    "zod": "npm:zod@^3.24.4",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.5.0",
    "hono-rate-limiter": "npm:hono-rate-limiter@^0.4.2",
    "youtubei.js": "npm:youtubei.js@^13.4.0",
    "lodash": "https://cdn.skypack.dev/lodash"
  },
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-read --allow-env --unstable-kv --allow-write --allow-import src/main.ts",
    "start": "deno run --allow-net --allow-read --allow-env --unstable-kv --allow-write --allow-import src/main.ts",
    "deploy": "deployctl deploy --prod"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "deploy": {
    "project": "bb79a90e-e05d-4b0d-aa11-0ef70f40e56c",
    "exclude": ["**/node_modules"],
    "include": [],
    "entrypoint": "src/main.ts"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any"]
    }
  }
}
